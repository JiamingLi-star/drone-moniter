{
  "name": "@ezuikit/player-ezopen",
  "version": "8.2.5",
  "description": "萤石ezopne协议播放器",
  "main": "dist/index.js",
  "umd": "dist/index.umd.js",
  "types": "dist/types/index.d.ts",
  "module": "dist/index.esm.js",
  "scripts": {
    "dev": "npm run clean && cross-env NODE_ENV=development rollup --config rollup.config.js --bundleConfigAsCjs --watch",
    "build": "npm run clean && cross-env NODE_ENV=production rollup --config rollup.config.js --bundleConfigAsCjs",
    "test": "jest",
    "prebuild": "npm run clean",
    "clean": "rimraf docs dist coverage",
    "lint": "eslint .",
    "fmt": "prettier --write \"packages/**/src/**/*.{js,ts,json,md}\"",
    "docs": "typedoc --tsconfig typedoc.config.js --exclude **/*.js"
  },
  "files": [
    "dist",
    "!dist/**/*.js.map"
  ],
  "keywords": [],
  "author": "Ezviz-OpenBiz",
  "license": "MIT",
  "dependencies": {
    "@ezuikit/player-plugin-record": "8.2.5-beta.8",
    "@ezuikit/utils-i18n": "2.0.0",
    "@ezuikit/utils-logger": "2.0.0",
    "@ezuikit/utils-service": "1.0.1",
    "@ezuikit/utils-tools": "2.0.1",
    "dayjs": "^1.11.10",
    "deepmerge": "^4.3.1",
    "eventemitter3": "^5.0.1",
    "screenfull": "^5.2.0",
    "ua-parser-js": "1.0.37"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ezviz-OpenBiz/EZUIKit-player-ezopen.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
