{
  "name": "@ezuikit/utils-service",
  "version": "1.0.1",
  "description": "service utils",
  "main": "dist/index.js",
  "umd": "dist/index.umd.js",
  "types": "dist/types/index.d.ts",
  "module": "dist/index.esm.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development rollup --config rollup.config.js --bundleConfigAsCjs --watch",
    "build": "cross-env NODE_ENV=production npm run clean && rollup --config rollup.config.js --bundleConfigAsCjs && npm run types && npm run docs",
    "types": "tsc src/index.ts --esModuleInterop --emitDeclarationOnly --declaration --declarationDir dist/types",
    "docs": "rimraf docs && typedoc --out docs src",
    "prebuild": "npm run clean",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "clean": "rimraf docs dist coverage",
    "lint": "eslint .",
    "fmt": "prettier --write \"packages/**/src/**/*.{js,ts,json,md}\""
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "service",
    "ezuikit",
    "ezuikit-utils",
    "utils"
  ],
  "author": "Ezviz-OpenBiz",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@ezuikit/utils-tools": "^1.0.1",
    "dayjs": "^1.11.10"
  }
}
